; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cook Halloween"
run/main_scene="res://Levels/TestStage.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

CookInput="*res://addons/cookpot/CookInput.tscn"
CookSFX="*res://addons/cookpot/CookSFX.tscn"
CookGFX="*res://addons/cookpot/CookGFX.tscn"
CookMusic="*res://addons/cookpot/CookMusic.tscn"
CookSave="*res://addons/cookpot/CookSave.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[editor_plugins]

enabled=PackedStringArray("res://addons/cookpot/plugin.cfg", "res://addons/goshapes/plugin.cfg")

[physics]

3d/default_gravity=40.0
